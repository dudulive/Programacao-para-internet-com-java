<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
	version="2.2">

	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>actionVoltarMenu</from-outcome>
			<to-view-id>/index.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>index.xhtml</display-name>
		<from-view-id>/index.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>actionListarProduto</from-outcome>
			<to-view-id>/produto/lista_produto.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>index.xhtml</display-name>
		<from-view-id>/index.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>actionListarTipo</from-outcome>
			<to-view-id>/tipo/lista_tipo.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>produto/lista_produto.xhtml</display-name>
		<from-view-id>/produto/lista_produto.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>actionManterProduto</from-outcome>
			<to-view-id>/produto/form_produto.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>tipo/lista_tipo.xhtml</display-name>
		<from-view-id>/tipo/lista_tipo.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>actionManterTipo</from-outcome>
			<to-view-id>/tipo/form_tipo.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>tipo/form_tipo.xhtml</display-name>
		<from-view-id>/tipo/form_tipo.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>actionListarAlteracaoTipo</from-outcome>
			<to-view-id>/tipo/lista_tipo.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>produto/form_produto.xhtml</display-name>
		<from-view-id>/produto/form_produto.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>actionListarAlteracaoProduto</from-outcome>
			<to-view-id>/produto/lista_produto.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>produto/lista_produto.xhtml</display-name>
		<from-view-id>/produto/lista_produto.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>actionExcluirProduto</from-outcome>
			<to-view-id>/produto/lista_produto.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>tipo/lista_tipo.xhtml</display-name>
		<from-view-id>/tipo/lista_tipo.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>actionExcluirTipo</from-outcome>
			<to-view-id>/tipo/lista_tipo.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>tipo/tipoSucesso.xhtml</display-name>
		<from-view-id>/tipo/tipoSucesso.xhtml</from-view-id>
		<navigation-case>
		    <from-outcome>actionListarAlteracaoTipo</from-outcome>
			<to-view-id>/tipo/lista_tipo.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>produto/produtoSucesso.xhtml</display-name>
		<from-view-id>/produto/produtoSucesso.xhtml</from-view-id>
		<navigation-case>
		    <from-outcome>actionListarAlteracaoProduto</from-outcome>
			<to-view-id>/produto/lista_produto.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
</faces-config>
